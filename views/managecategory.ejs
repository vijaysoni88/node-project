<% include adminheader %>
<section id="contact">
    <div class="container mt-5">
  
      <div class="section-header mt-5 pb-2">
        <h3>Add Category</h3>
      </div>
  
      <div class="row wow fadeInUp" style="visibility: visible; animation-name: fadeInUp;">
        
        <div class="col-lg-6 offset-lg-3 shadow p-5">
            <b class="text-warning"><%= msg%></b>
            <form method="POST" action="" enctype="multipart/form-data">
              <div class="form-group">
                <label for="catnm">Category Name</label>
                <input type="text" name="catnm" class="form-control">
              </div>
              <div class="form-group">
                <label for="caticon">Category Icon</label>
                <input type="file" name="caticon" class="form-control" >
              </div>
             <div class="text-right">
              <button type="submit" class="btn btn-primary">Add Category</button>
             </div>
            </form>
          </div>
        </div>
  
      </div>
  
    </div>
  </section>
<% include footer %>
